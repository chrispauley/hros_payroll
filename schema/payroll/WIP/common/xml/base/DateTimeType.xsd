<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:hros="http://hropenstandards.org/xml/" targetNamespace="http://hropenstandards.org/xml/" version="4.1.0" id="DateTimeType" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:simpleType name="DateTimeType">
    <xsd:annotation>
      <xsd:documentation>DateTime representation according ISO 8601 standard</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:dateTime" />
  </xsd:simpleType>
  <xsd:element name="DateTime" type="hros:DateTimeType" />
</xsd:schema>